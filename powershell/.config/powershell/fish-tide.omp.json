{
	"$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh3/main/themes/schema.json",
	"blocks": [
		{
			"type": "prompt",
			"alignment": "left",
			"segments": [
				// {
				// 	"type": "os",
				// 	"style": "diamond",
				// 	"foreground": "#000000",
				// 	"background": "#d3d7cf",
				// 	"leading_diamond": "",
				// 	"trailing_diamond": ""
				// },
				// {
				// 	"type": "path",
				// 	"style": "powerline",
				// 	"powerline_symbol": "",
				// 	"foreground": "#e4e4e4",
				// 	"background": "#3465a4",
				// 	"properties": {
				// 		"prefix": " \uF07C ",
				// 		"home_icon": "~",
				// 		"style": "full"
				// 	}
				// },
				{
					"type": "path",
					"style": "diamond",
					"background": "#444444",
					"foreground": "#E4E4E4",
					"leading_diamond": "",
				 	"trailing_diamond": "\ue0bc",
					"properties": {
						"prefix": " ",
						"postfix": "",
						"home_icon": " ~",
						"folder_separator_icon": " / ",
						"style": "full"
					}
				},
				{
					"type": "git",
					"style": "diamond",
					"leading_diamond": "\ue0ba ",
					"trailing_diamond": "",
					"foreground": "#000000",
					"background": "#268bd2",
					"properties": {
						"branch_icon": "\uF126 ",
						"display_stash_count": true,
						"display_upstream_icon": true,
						"status_colors_enabled": true,
						"local_changes_color": "#c4a000",
						"ahead_and_behind_color": "#f26d50",
						"behind_color": "#4e9a06",
						"ahead_color": "#89d1dc",
						"stash_count_icon": "\uF692 "
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "right",
			"segments": [
				{
					"type": "exit",
					"style": "powerline",
					"powerline_symbol": "",
					"invert_powerline": true,
					"foreground": "#d3d7cf",
					"background": "#000000",
					"properties": {
						"always_enabled": true,
						"display_exit_code": true,
						"error_color": "#cc2222",
						"color_background": true,
						"success_icon": "✔"
					}
				},
				{
					"type": "dotnet",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#111111",
					"background": "#600186",
					"leading_diamond": "",
					"trailing_diamond": "",
					"properties": {
						"display_version": true,
						"prefix": "  "
					}
				},
				{
					"type": "node",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#111111",
					"background": "#0d8a01",
					"properties": {
						"postfix": " \uF898 ",
						"display_version": true
					}
				},
				{
					"type": "go",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#111111",
					"background": "#00badb",
					"properties": {
						"postfix": " \uE627 ",
						"display_version": true
					}
				},
				{
					"type": "julia",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#111111",
					"background": "#00218b",
					"properties": {
						"postfix": " \uE624 ",
						"display_version": true
					}
				},
				{
					"type": "python",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#00488b",
					"background": "#e7eb00",
					"properties": {
						"postfix": " \uE235 ",
						"display_version": true,
						"display_mode": "files",
						"display_virtual_env": false
					}
				},
				{
					"type": "ruby",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#ffffff",
					"background": "#AE1401",
					"properties": {
						"postfix": " \uE791 ",
						"display_version": true,
						"display_mode": "files"
					}
				},
				{
					"type": "azfunc",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#ffffff",
					"background": "#FEAC19",
					"properties": {
						"postfix": " \uf0e7",
						"display_version": false,
						"display_mode": "files"
					}
				},
				{
					"type": "aws",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#ffffff",
					"background_templates": [
						"{{if contains \"default\" .Profile}}#FFA400{{end}}",
						"{{if contains \"jan\" .Profile}}#f1184c{{end}}"
					],
					"properties": {
						"postfix": " \uE7AD ",
						"display_default": false
					}
				},
				{
					"type": "root",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#111111",
					"background": "#ffff66",
					"properties": {
						"root_icon": "\uF0AD"
					}
				},
				{
					"type": "executiontime",
					"style": "powerline",
					"powerline_symbol": "\ue0ba ",
					"invert_powerline": true,
					"foreground": "#000000",
					"background": "#b98c30",
					"properties": {
						"postfix": " \uF252 "
					}
				},
				{
					"type": "time",
					"style": "diamond",
					"invert_powerline": true,
					"trailing_diamond": "",
					"background": "#d3d7cf",
					"foreground": "#000000",
					"properties": {
						"postfix": " \uF017 "
					}
				}
			]
		},
		{
			"type": "prompt",
			"alignment": "left",
			"newline": true,
			"segments": [{
				"type": "exit",
				"style": "plain",
				"foreground": "#0c55c4",
				"properties": {
					"prefix": "\u276F",
					"always_enabled": true,
					"error_color": "#FF5252",
					"display_exit_code": false
				}
			}]
		}
	],
	"final_space": true,
	"console_title": true,
	"console_title_style": "template",
	"console_title_template": "{{ .Shell }} in {{ .Folder }}"
}
