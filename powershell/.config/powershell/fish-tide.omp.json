{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "transient_prompt": {
    "template": "‚ùØ",
    "foreground": "#0c55c4",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#FF5252{{ end }}"
    ]
  },
  "console_title_template": "{{ .Shell }} in {{ .Folder }}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "folder_separator_icon": "/",
            "home_icon": "~",
            "style": "full"
          },
          "leading_diamond": "\ue0b6",
          "template": "{{ .Path }}{{ if .Segments.Git }}{{ else }}<background,transparent>\ue0b0</>{{ end }}",
          "foreground": "#E4E4E4",
          "background": "#444444",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          },
          "leading_diamond": "<background,parentBackground>\ue0ba</>",
          "trailing_diamond": "\ue0b0",
          "template": "{{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044{{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046{{ .Staging.String }}{{ end }}",
          "foreground": "#000000",
          "background": "#268bd2",
          "type": "git",
          "style": "diamond",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#c4a000{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#4e9a06{{ end }}",
            "{{ if gt .Behind 0 }}#89d1dc{{ end }}"
          ]
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none",
            "display_exit_code": true
          },
          "leading_diamond": "\ue0b2",
          "template": "{{ if gt .Code 0 }}\uf00d {{ .Meaning }}{{ else }}‚úî{{ end }}",
          "foreground": "#d3d7cf",
          "background": "#000000",
          "type": "text",
          "style": "diamond",
          "background_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "\ue70c {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#111111",
          "background": "#600186",
          "type": "dotnet",
          "style": "powerline"
        },
        {
          "properties": {
            "fetch_version": true
          },
          "leading_diamond": "<parentBackground></>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .PackageManagerIcon }}{{ .PackageManagerIcon }}{{ end }}Óúò {{ .Full }}",
          "foreground": "#111111",
          "background": "#0d8a01",
          "type": "node",
          "style": "powerline"
        },
        {
          "type": "nx",
          "style": "powerline",
          "powerline_symbol": "<backgroundColor>\ue0bc</>",
          "foreground": "#000000",
          "background": "#1976d2",
          "template": "Nx{{ .Full }}"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .Error }} {{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627",
          "foreground": "#111111",
          "background": "#00badb",
          "type": "go",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "fetch_version": true
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .Error }} {{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624",
          "foreground": "#111111",
          "background": "#00218b",
          "type": "julia",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true,
            "fetch_virtual_env": false
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .Error }} {{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }} \ue235",
          "foreground": "#00488b",
          "background": "#e7eb00",
          "type": "python",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": true
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .Error }} {{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791",
          "foreground": "#ffffff",
          "background": "#AE1401",
          "type": "ruby",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_mode": "files",
            "fetch_version": false
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "{{ if .Error }} {{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7",
          "foreground": "#ffffff",
          "background": "#FEAC19",
          "type": "azfunc",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none",
            "display_default": false
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue8ad",
          "foreground": "#ffffff",
          "type": "aws",
          "style": "powerline",
          "background_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ]
        },
        {
          "properties": {
            "cache_duration": "none",
            "root_icon": "\uf0ad"
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": "\uf0e7",
          "foreground": "#111111",
          "background": "#ffff66",
          "type": "root",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "leading_diamond": "<parentBackground>\ue0bc</>",
          "powerline_symbol": "\ue0bc",
          "template": " {{ .FormattedMs }} \uf252",
          "foreground": "#000000",
          "background": "#b98c30",
          "type": "executiontime",
          "style": "powerline"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "trailing_diamond": "\ue0b4",
          "template": "{{ .CurrentDate | date .Format }} \uf017 ",
          "foreground": "#000000",
          "background": "#d3d7cf",
          "type": "time",
          "style": "diamond"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true,
            "cache_duration": "none"
          },
          "template": "‚ùØ",
          "foreground": "#0c55c4",
          "type": "text",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#FF5252{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
